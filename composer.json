{
  "name": "mimmi20/laminasviewrenderer-helper-partialrenderer",
  "description": "Provides a render helper for Laminas View",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/mimmi20/laminasviewrenderer-helper-partialrenderer",
  "support": {
    "issues": "https://github.com/mimmi20/laminasviewrenderer-helper-partialrenderer/issues",
    "source": "https://github.com/mimmi20/laminasviewrenderer-helper-partialrenderer"
  },
  "require": {
    "php": "~8.3.0 || ~8.4.0 || ~8.5.0",
    "mezzio/mezzio-laminasviewrenderer": "^2.16.0",
    "psr/container": "^1.1.2 || ^2.0.2"
  },
  "require-dev": {
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-simplexml": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "infection/infection": "^0.29.14",
    "laminas/laminas-servicemanager": "^3.22.1",
    "laminas/laminas-view": "^2.36.0",
    "mimmi20/coding-standard": "^6.0.25",
    "nikic/php-parser": "^5.4.0",
    "phpstan/extension-installer": "^1.4.3",
    "phpstan/phpstan": "^2.1.11",
    "phpstan/phpstan-deprecation-rules": "^2.0.1",
    "phpunit/phpunit": "^12.1.2",
    "rector/rector": "^2.0.11",
    "rector/type-perfect": "^2.0.2",
    "shipmonk/composer-dependency-analyser": "^1.8.2",
    "symfony/process": "^7.2.5",
    "symplify/phpstan-rules": "^14.6.2",
    "tomasvotruba/cognitive-complexity": "^1.0.0",
    "tomasvotruba/type-coverage": "^2.0.2",
    "tomasvotruba/unused-public": "^2.0.1"
  },
  "suggest": {
    "ext-intl": "required to use the Translator",
    "laminas/laminas-servicemanager": "to use the helper factories"
  },
  "autoload": {
    "psr-4": {
      "Mimmi20\\LaminasView\\Helper\\PartialRenderer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mimmi20Test\\LaminasView\\Helper\\PartialRenderer\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "infection/extension-installer": true,
      "phpstan/extension-installer": true
    },
    "lock": false,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laminas": {
      "config-provider": "Mimmi20\\LaminasView\\Helper\\PartialRenderer\\ConfigProvider"
    }
  }
}
