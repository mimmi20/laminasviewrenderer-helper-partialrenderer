{
    "name": "mimmi20/laminasviewrenderer-helper-partialrenderer",
    "description": "Provides a render helper for Laminas View",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Thomas MÃ¼ller",
            "homepage": "https://github.com/mimmi20",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/mimmi20/laminasviewrenderer-helper-partialrenderer",
    "support": {
        "issues": "https://github.com/mimmi20/laminasviewrenderer-helper-partialrenderer/issues",
        "source": "https://github.com/mimmi20/laminasviewrenderer-helper-partialrenderer"
    },
    "require": {
        "php": "^8.1.0",
        "mezzio/mezzio-laminasviewrenderer": "^2.15.0"
    },
    "require-dev": {
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-simplexml": "*",
        "ext-tokenizer": "*",
        "ext-xml": "*",
        "ext-xmlwriter": "*",
        "infection/infection": "^0.27.9",
        "laminas/laminas-config": "^3.9.0",
        "laminas/laminas-escaper": "^2.13.0",
        "laminas/laminas-i18n": "^2.26.0",
        "laminas/laminas-modulemanager": "^2.15.0",
        "laminas/laminas-navigation": "^2.19.1",
        "laminas/laminas-permissions-acl": "^2.16.0",
        "laminas/laminas-servicemanager": "^3.22.1",
        "laminas/laminas-view": "^2.33.0",
        "mimmi20/coding-standard": "^5.2.9",
        "mimmi20/laminas-form-element-group": "^3.0.6",
        "mimmi20/laminas-form-element-links": "^2.0.5",
        "mimmi20/laminas-form-element-paragraph": "^2.0.5",
        "mimmi20/mezzio-generic-authorization": "^3.0.3",
        "mimmi20/mezzio-navigation": "^3.0.4",
        "phpstan/extension-installer": "^1.3.1",
        "phpstan/phpstan": "^1.10.55",
        "phpstan/phpstan-deprecation-rules": "^1.1.4",
        "phpstan/phpstan-phpunit": "^1.3.15",
        "phpunit/phpunit": "^10.5.7",
        "rector/rector": "^0.19.0"
    },
    "suggest": {
        "ext-intl": "required to use the Translator",
        "laminas/laminas-config": "to provide configuration (optional, as arrays and Traversables are also allowed)",
        "laminas/laminas-servicemanager": "to use the helper factories"
    },
    "autoload": {
        "psr-4": {
            "Mimmi20\\LaminasView\\Helper\\PartialRenderer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mimmi20Test\\LaminasView\\Helper\\PartialRenderer\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "lock": false,
        "sort-packages": true,
        "allow-plugins": {
            "laminas/laminas-dependency-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true,
            "infection/extension-installer": true
        }
    },
    "extra": {
        "laminas": {
            "config-provider": "Mimmi20\\LaminasView\\Helper\\PartialRenderer\\ConfigProvider"
        }
    }
}
